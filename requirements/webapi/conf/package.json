    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/websockets": "^9.3.9",
    "@prisma/client": "^4.11.0",
    "@socket.io/redis-adapter": "^8.1.0",
    "@types/socket.io-client": "^3.0.0",
    "add": "^2.0.6",
    "argon2": "^0.30.3",
